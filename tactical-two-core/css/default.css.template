.reviews-stars:before {
    font-family: "fontello";
    content: "\e803 \e803 \e803 \e803 \e803";
    color: #b8b8b8;
}
.rating:hover .rating-star:hover,
.rating:hover .rating-star:hover ~ .rating-star,
.rating-input:checked ~ .rating-star,
.reviews-stars {
    color: #[gord_stars];
    letter-spacing: 3px;
}

.product-item .reviews span.reviews-count{
    display: none;
}

.product-item .name a {
	font-weight: 600;
    color: #[gord_bloglist_product_name];
}
.product-item:hover .name a {
	color: #[gord_bloglist_product_name_hover];
}
.product-item del.regular-price {
    text-decoration: line-through;
}
.sale-price {
    color: #[gord_theme_color];
}
.product-item .on-sale-badge{
    color: #[gord_saletag_text];
